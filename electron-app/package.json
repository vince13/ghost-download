{
  "name": "ghost",
  "version": "0.1.1",
  "description": "Ghost Protocol - AI-powered real-time conversation coaching",
  "author": {
    "name": "Ghost Protocol",
    "email": "support@ghostprotocol.ai"
  },
  "homepage": "https://ghost-green.vercel.app",
  "private": true,
  "type": "module",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:electron": "vite build --mode electron",
    "preview": "vite preview",
    "lint": "eslint .",
    "electron": "electron .",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:build": "npm run build:electron && electron-builder --config electron-builder.config.cjs",
    "electron:build:mac": "../scripts/cleanup-dmg.sh && npm run build:electron && electron-builder --config electron-builder.config.cjs --mac && ../scripts/remove-quarantine.sh",
    "electron:build:win": "npm run build:electron && electron-builder --config electron-builder.config.cjs --win",
    "electron:build:linux": "npm run build:electron && electron-builder --config electron-builder.config.cjs --linux --x64",
    "electron:build:all": "npm run build:electron && electron-builder --config electron-builder.config.cjs --mac --win --linux --x64 && ../scripts/remove-quarantine.sh",
    "remove-quarantine": "../scripts/remove-quarantine.sh",
    "copy-downloads": "node ../scripts/copy-downloads.js",
    "build:mac:deploy": "npm run electron:build:mac && npm run copy-downloads",
    "build:win:deploy": "npm run electron:build:win && npm run copy-downloads",
    "build:linux:deploy": "npm run electron:build:linux && npm run copy-downloads"
  },
  "dependencies": {
    "@pinecone-database/pinecone": "^6.1.3",
    "@vapi-ai/web": "^2.5.1",
    "clsx": "^2.1.1",
    "firebase": "^11.0.0",
    "firebase-admin": "^13.6.0",
    "lucide-react": "^0.453.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.9.6",
    "stripe": "^20.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.19",
    "concurrently": "^9.1.0",
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8",
    "eslint": "^9.12.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "postcss": "^8.4.45",
    "tailwindcss": "^3.4.13",
    "vite": "^5.4.0",
    "wait-on": "^8.0.1"
  }
}
